				"cell_type": "code",
				"source": [
					"entity_stage_name = input_arguments.get(\"entity_stage_name\")\n",
					"etl_process_class = ETLProcessFactory.get(entity_stage_name)\n",
					"etl_process_instance = etl_process_class(spark)"
