				"cell_type": "code",
				"source": [
					"from odw.core.etl.etl_process_factory import ETLProcessFactory\n",
					"from odw.core.io.data_io_factory import DataIOFactory\n",
					"from pyspark.sql import SparkSession\n",
					"from typing import Dict, Any"
				],
				"execution_count": null
			},
			{
				"cell_type": "markdown",
				"metadata": {
					"nteract": {
						"transient": {
							"deleting": false
						}
					}
				},
				"source": [
					"# Notebook parameters"
				]
			},
			{
				"cell_type": "code",
				"metadata": {
					"tags": [
						"parameters"
					]
				},
				"source": [
					"input_arguments: Dict[str, Any] = {\n",
					"    \"source_data\": {\n",
					"        \"someblob\": {\n",
					"            \"storage_name\": \"\",\n",
					"            \"container_name\": \"odw-raw\",\n",
					"            \"blob_path\": \"\",\n",
					"            \"file_format\": \"json\"\n",
					"        }\n",
					"    }\n",
					"}"
				],
				"execution_count": null
			},
			{
				"cell_type": "markdown",
				"metadata": {
					"nteract": {
						"transient": {
							"deleting": false
						}
					}
				},
				"source": [
					"# Validate notebook parameters"
				]
			},
			{
				"cell_type": "code",
				"source": [
					"if \"source_data\" not in input_arguments:\n",
					"    raise ValueError(\"py_etl_ochestrator expects a 'source_data' property to be supplied, but this was missing\")"
				],
				"execution_count": null
			},
			{
				"cell_type": "markdown",
				"metadata": {
					"nteract": {
						"transient": {
							"deleting": false
						}
					}
				},
				"source": [
					"# Load data"
				]
			},
			{
				"cell_type": "code",
				"source": [
					"source_data = dict()\n",
					"for data_name, metadata in source_data.items():\n",
					"    storage_kind = metadata.get(\"storage_kind\", None)\n",
					"    data_format = metadata.get(\"data_format\", None)\n",
					"    if not storage_kind:\n",
					"        raise ValueError(f\"Expected a storage_kind parameter to be passed, but this was missing\")\n",
					"    if not data_format:\n",
					"        raise ValueError(f\"Expected a data_format parameter to be passed, but this was missing\")\n",
					"    data_io_inst = DataIOFactory.get(storage_kind)()\n",
					"    data = data_io_inst.read(**metadata)\n",
					"    source_data[data_name] = data\n",
					""
				],
				"execution_count": null
			},
			{
				"cell_type": "markdown",
				"metadata": {
					"nteract": {
						"transient": {
							"deleting": false
						}
					}
				},
				"source": [
					"# Load the relevant ETL Processor"
				]
			},
			{
				"cell_type": "code",
				"source": [
					"spark = SparkSession.builder.getOrCreate()\n",
					"etl_process_class = ETLProcessFactory.get(entity_stage_name)\n",
					"etl_process_instance = etl_process_class(spark)"
				],
				"execution_count": null
			},
			{
				"cell_type": "markdown",
				"metadata": {
					"nteract": {
						"transient": {
							"deleting": false
						}
					}
				},
				"source": [
					"# Perform ETL"
				]
			},
			{
				"cell_type": "code",
				"source": [
					"etl_results = etl_process_instance.process(\n",
					"    source_data=source_data,\n",
					"    **input_arguments,\n",
					"    spark=spark\n",
					")"
				],
				"execution_count": null
			},
			{
				"cell_type": "markdown",
				"metadata": {
					"nteract": {
						"transient": {
							"deleting": false
						}
					}
				},
				"source": [
					"# Write output data"
				]
			},
			{
				"cell_type": "code",
				"source": [
					"for result_name, result_metadata in etl_results.items():\n",
					"    result_data = result_metadata.get(\"data\")\n",
					"    destination_kind = metadata.get(\"storage_kind\", None)\n",
					"    destination_data_format = metadata.get(\"data_format\", None)\n",
					"    if not storage_kind:\n",
					"        raise ValueError(f\"Expected a storage_kind parameter to be returned by {etl_process_class}.process, but this was missing\")\n",
					"    if not data_format:\n",
					"        raise ValueError(f\"Expected a data_format parameter to be returned by {etl_process_class}.process, but this was missing\")\n",
					"    data_io_inst = DataIOFactory.get(storage_kind)()\n",
					"    data_io_inst.write(data=result_data, **metadata)"
				],
				"execution_count": null
			}
		]
