			{
				"cell_type": "code",
				"source": [
					"from pyspark.sql.types import DateType, TimestampType\n",
					"\n",
					"def apply_ancient_datetime_filter(df, cutoff=\"1900-01-01\"):\n",
					"    datetime_cols = [\n",
					"        f.name for f in df.schema.fields\n",
					"        if isinstance(f.dataType, (TimestampType, DateType))\n",
					"    ]\n",
					"\n",
					"    if not datetime_cols:\n",
					"        return df\n",
					"\n",
					"    filter_expr = \" AND \".join([\n",
					"        f\"({c} IS NULL OR {c} >= '{cutoff}')\"\n",
					"        for c in datetime_cols\n",
					"    ])\n",
					"\n",
					"    return df.filter(filter_expr)\n",
					"\n",
					"# Apply before timezone conversion\n",
					"filtered_df = apply_ancient_datetime_filter(view_df)"
				],
				"execution_count": 5
			},
