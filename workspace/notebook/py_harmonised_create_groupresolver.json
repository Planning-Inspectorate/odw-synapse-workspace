			{
				"cell_type": "code",
				"source": [
					"db_name = \"odw_harmonised_db\"\n",
					"table_name = \"GroupResolver\"  "
				],
				"execution_count": 8
			},
			{
				"cell_type": "code",
				"source": [
					"spark.sql(f\"CREATE DATABASE IF NOT EXISTS {db_name}\")"
				],
				"execution_count": 9
			},
			{
				"cell_type": "code",
				"source": [
					"from pyspark.sql.types import StructType, StructField, StringType, TimestampType\n",
					"from pyspark.sql import functions as F\n",
					"\n",
					"schema = StructType([\n",
					"    StructField(\"caseReference\", StringType(), False),  \n",
					"    StructField(\"currentGroup\", StringType(), False),   \n",
					"    StructField(\"asOfTimestamp\", TimestampType(), False)\n",
					"])\n",
					"\n",
					"empty_df = spark.createDataFrame([], schema)\n",
					"\n",
					"spark.sql(f\"USE {db_name}\")\n",
					"if not spark._jsparkSession.catalog().tableExists(db_name, table_name):\n",
					"    (empty_df.write\n",
					"        .format(\"delta\")\n",
					"        .mode(\"overwrite\")\n",
					"        .saveAsTable(f\"{db_name}.{table_name}\"))\n",
					""
				],
				"execution_count": 10
			},
			{
				"cell_type": "code",
				"metadata": {
					"microsoft": {
						"language": "sparksql"
					},
					"collapsed": false
				},
				"source": [
					"%%sql\n",
					"WITH a AS (\n",
					"  SELECT DISTINCT TRIM(UPPER(caseReference)) AS k\n",
					"  FROM odw_harmonised_db.appeal_has\n",
					"  WHERE TRIM(UPPER(IsActive))='Y'\n",
					"),\n",
					"b AS (\n",
					"  SELECT DISTINCT TRIM(UPPER(caseReference)) AS k\n",
					"  FROM odw_harmonised_db.appeal_s78\n",
					"  WHERE TRIM(UPPER(IsActive))='Y'\n",
					"),\n",
					"both AS (\n",
					"  SELECT k FROM a INTERSECT SELECT k FROM b\n",
					")\n",
					"SELECT\n",
					"  both.k                                      AS case_ref_key,\n",
					"  r.currentGroup                              AS resolver_group\n",
					"FROM both\n",
					"LEFT JOIN odw_harmonised_db.GroupResolver r\n",
					"  ON TRIM(UPPER(r.caseReference)) = both.k\n",
					"ORDER BY resolver_group;\n",
					""
				],
				"execution_count": 12
			},
			{
				"cell_type": "code",
				"source": [
					"spark.sql(f\"DELETE FROM {db_name}.{table_name}\")"
