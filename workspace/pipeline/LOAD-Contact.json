{
	"name": "LOAD-Contact",
	"properties": {
		"activities": [
			{
				"name": "Load Address",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFolderPath": "MPESC-TRANSFORM/Address/ContactAddress.csv/",
							"wildcardFileName": "*.csv",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "upsert",
						"upsertSettings": {
							"useTempDB": true,
							"keys": [
								"id"
							]
						},
						"sqlWriterUseTableLock": false,
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "addressLine1"
								},
								"sink": {
									"name": "line1"
								}
							},
							{
								"source": {
									"name": "addressLine2"
								},
								"sink": {
									"name": "line2"
								}
							},
							{
								"source": {
									"name": "townCity"
								},
								"sink": {
									"name": "townCity"
								}
							},
							{
								"source": {
									"name": "county"
								},
								"sink": {
									"name": "county"
								}
							},
							{
								"source": {
									"name": "postcode"
								},
								"sink": {
									"name": "postcode"
								}
							},
							{
								"source": {
									"name": "caseId"
								},
								"sink": {
									"name": "legacyCaseId"
								}
							},
							{
								"source": {
									"name": "addressId"
								},
								"sink": {
									"name": "id"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "ContactAddress",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "Mpesc_Address",
						"type": "DatasetReference"
					}
				]
			},
			{
				"name": "Load Contact",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Load Address",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFolderPath": "MPESC-TRANSFORM/CaseInvolvement/CaseInvolvement_Contacts.csv",
							"wildcardFileName": "*.csv",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "Contact",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "Horizon Migration/MPESC-LOAD"
		},
		"annotations": []
	}
}