{
	"name": "LOAD-ADDRESS",
	"properties": {
		"activities": [
			{
				"name": "Load Address",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "DelimitedTextSource",
						"storeSettings": {
							"type": "AzureBlobFSReadSettings",
							"recursive": true,
							"wildcardFolderPath": "MPESC-TRANSFORM/case-merged-sample-row",
							"wildcardFileName": "*.csv",
							"enablePartitionDiscovery": false
						},
						"formatSettings": {
							"type": "DelimitedTextReadSettings"
						}
					},
					"sink": {
						"type": "AzureSqlSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "ID"
								},
								"sink": {
									"name": "id"
								}
							},
							{
								"source": {
									"name": "Site Address Line 1"
								},
								"sink": {
									"name": "line1"
								}
							},
							{
								"source": {
									"name": "Site Address Line 2"
								},
								"sink": {
									"name": "line2"
								}
							},
							{
								"source": {
									"name": "Site Address Town"
								},
								"sink": {
									"name": "townCity"
								}
							},
							{
								"source": {
									"name": "Site Address County"
								},
								"sink": {
									"name": "county"
								}
							},
							{
								"source": {
									"name": "Site Address Postcode"
								},
								"sink": {
									"name": "postcode"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "Address",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "AzureSqlTable4",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "Horizon Migration/MPESC-LOAD"
		},
		"annotations": []
	}
}