								{
									"activity": "Log_No_Entities",
									"dependencyConditions": [
										"Succeeded"
									]
								}
							],
							"policy": {
								"secureOutput": false,
								"secureInput": false
							},
							"userProperties": [],
							"typeProperties": {
								"variableName": "transformStatus",
								"value": {
									"value": "@json('[]')\n",
									"type": "Expression"
								}
							}
						}
					]
				}
			},
