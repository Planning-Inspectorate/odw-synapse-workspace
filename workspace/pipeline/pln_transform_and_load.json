		"activities": [
			{
				"name": "Guard_NoEntities",
				"type": "IfCondition",
				"dependsOn": [],
				"userProperties": [],
				"typeProperties": {
					"expression": {
						"value": "@and(\n  equals(length(pipeline().parameters.sbEntities), 0),\n  equals(length(pipeline().parameters.hzEntities), 0)\n)\n",
						"type": "Expression"
					},
					"ifTrueActivities": [
						{
							"name": "Log_No_Entities",
