					"Horizon_TypeOfDevelopment",
					"Horizon_TypeOfDocument",
					"Horizon_TypeOfDocumentGroup",
					"Horizon_TypeOfEvent",
					"Horizon_TypeOfExamination",
					"Horizon_TypeOfGroundForAppeal",
					"Horizon_TypeOfInvolvement",
