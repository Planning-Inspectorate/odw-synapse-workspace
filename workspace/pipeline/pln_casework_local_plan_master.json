						"activity": "pln_casework_local_plan_main",
						"dependencyConditions": [
							"Failed"
						]
					}
				],
				"policy": {
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"pipeline": {
						"referenceName": "pln_log_to_appins",
						"type": "PipelineReference"
					},
					"waitOnCompletion": true,
					"parameters": {
						"Stage": "Fail",
						"PipelineName": {
							"value": "@activity('pln_casework_local_plan_main').output.pipelineName\n",
							"type": "Expression"
						},
						"PipelineRunID": {
							"value": "@activity('pln_casework_local_plan_main').output.pipelineRunId",
							"type": "Expression"
						},
						"StartTime": {
							"value": "@activity('pln_casework_local_plan_main').ExecutionStartTime",
							"type": "Expression"
						},
						"EndTime": {
							"value": "@activity('pln_casework_local_plan_main').ExecutionEndTime",
							"type": "Expression"
						},
						"ErrorMessage": {
							"value": "@activity('pln_casework_local_plan_main').Error.message",
							"type": "Expression"
						},
						"StatusMessage": {
							"value": "@activity('pln_casework_local_plan_main').Status",
							"type": "Expression"
						},
						"PipelineTriggeredbyPipelineName": {
							"value": "@activity('pln_casework_local_plan_main').PipelineName",
							"type": "Expression"
						},
						"PipelineTriggeredbyPipelineRunID": {
							"value": "@activity('pln_casework_local_plan_main').PipelineRunId",
							"type": "Expression"
						},
						"PipelineExecutionTimeInSec": {
							"value": "@activity('pln_casework_local_plan_main').Duration",
							"type": "Expression"
						},
						"ActivityType": {
							"value": "@activity('pln_casework_local_plan_main').ActivityType",
							"type": "Expression"
						},
						"DurationSeconds": {
							"value": "@div(sub(ticks(utcNow()), ticks(variables('start_time'))), 10000000)",
							"type": "Expression"
						},
						"AppInsCustomEventName": "ODW_Local_Plan_Master_Pipeline_Logs",
						"StatusCode": {
							"value": "@activity('pln_casework_local_plan_main').StatusCode",
							"type": "Expression"
						},
						"AppInsIKey": {
							"value": "@variables('apps_insights_ikey')",
							"type": "Expression"
						}
					}
				}
			},
			{
				"name": "Send Casework Local Plan failed",
				"type": "ExecutePipeline",
				"dependsOn": [
					{
						"activity": "pln_casework_local_plan_main",
						"dependencyConditions": [
							"Failed"
						]
					}
				],
				"policy": {
					"secureInput": true
				},
				"userProperties": [],
