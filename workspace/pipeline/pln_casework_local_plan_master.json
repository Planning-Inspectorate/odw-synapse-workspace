							"value": "@variables('resource_group')",
							"type": "Expression"
						},
						"pipelineRunId": {
							"value": "@pipeline().RunId",
							"type": "Expression"
						},
						"teamsWebhookUrl": {
							"value": "@variables('webhook_url')",
							"type": "Expression"
						},
						"activityName": {
							"value": "@pipeline().Pipeline",
							"type": "Expression"
						},
						"activityMessage": "Casework Local Plan data failed",
						"activityDuration": {
							"value": "@concat(string(div(div(sub(ticks(formatDateTime(utcNow(),'yyyy-MM-dd HH:mm:sss')),ticks(formatDateTime(variables('start_time'),'yyyy-MM-dd HH:mm:sss'))),600000000),60)),'H:', \nstring(mod(div(sub(ticks(formatDateTime(utcNow(),'yyyy-MM-dd HH:mm:sss')),ticks(formatDateTime(variables('start_time'),'yyyy-MM-dd HH:mm:sss'))),600000000),60)),'M:00s')",
							"type": "Expression"
						},
						"activityStatus": "Failed",
						"Colour": {
							"value": "@variables('failed_colour')",
							"type": "Expression"
						},
						"Image": {
							"value": "@variables('failed_image')",
							"type": "Expression"
						},
						"Message_title": "ODW - Casework Local Plan Master pipeline failed",
						"Message_subtitle": "ODW failed to load data from Casework Local Plan"
					}
				}
			},
			{
				"name": "Record Casework Local Plans pipeline completed",
				"type": "ExecutePipeline",
				"dependsOn": [
					{
						"activity": "pln_casework_local_plan_main",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"secureInput": false
				},
